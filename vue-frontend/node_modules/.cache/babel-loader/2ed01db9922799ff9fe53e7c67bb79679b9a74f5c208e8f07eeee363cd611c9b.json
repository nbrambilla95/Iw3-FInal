{"ast":null,"code":"import ChoferService from '@/services/ChoferService';\nexport default {\n  name: '',\n  data() {\n    return {\n      choferes: [],\n      token: '',\n      requestComplete: false\n    };\n  },\n  computed: {\n    async filteredChoferes() {\n      filteredChoferes = await choferes.filter(chofer => chofer.nombre.toLowerCase().includes(this.search.toLowerCase()));\n      return filteredChoferes;\n    }\n  },\n  methods: {\n    async getChoferes() {\n      try {\n        // Obtener un token llamando al método \"getTokens\" importado\n\n        // Llamar al método \"getCamiones\" del servicio de camión, pasando el token como un parámetro\n        this.choferes = await ChoferService.getChoferes(this.token);\n        this.requestComplete = true;\n      } catch (error) {\n        console.error(error);\n      }\n    }\n  }\n  // created(){\n  //      this.getCamiones()\n  // }\n};","map":{"version":3,"mappings":"AAkCA,OAAOA,aAAY,MAAO,0BAA0B;AAEpD,eAAe;EACXC,IAAI,EAAE,EAAE;EACRC,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,eAAe,EAAE;IACrB;EACJ,CAAC;EACDC,QAAQ,EAAE;IACN,MAAMC,gBAAgB,GAAG;MACrBA,gBAAe,GAAK,MAAMJ,QAAQ,CAACK,MAAM,CAACC,MAAK,IAAKA,MAAM,CAACC,MAAM,CAACC,WAAW,EAAE,CAACC,QAAQ,CAAC,IAAI,CAACC,MAAM,CAACF,WAAW,EAAE,CAAC,CAAC;MACpH,OAAOJ,gBAAe;IAC1B;EACJ,CAAC;EACDO,OAAO,EAAE;IAEL,MAAMC,WAAW,GAAG;MAChB,IAAI;QACA;;QAEA;QACA,IAAI,CAACZ,QAAO,GAAI,MAAMH,aAAa,CAACe,WAAW,CAAC,IAAI,CAACX,KAAK,CAAC;QAC3D,IAAI,CAACC,eAAc,GAAI,IAAI;MAC/B,EAAE,OAAOW,KAAK,EAAE;QACZC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACxB;IACJ;EACJ;EACA;EACA;EACA;AAEJ","names":["ChoferService","name","data","choferes","token","requestComplete","computed","filteredChoferes","filter","chofer","nombre","toLowerCase","includes","search","methods","getChoferes","error","console"],"sourceRoot":"","sources":["/home/nicolas/Downloads/repos/Iw3-FInal/vue-frontend/src/components/Chofer.vue"],"sourcesContent":["<template>\n    <input id=\"input_token\" type=\"text\" v-model=\"token\" />\n    <button id=\"id_button_choferes\" @click=\"getChoferes\" v-if=\"!requestComplete\">\n        Get choferes\n    </button>\n\n    <div class=\"container\" v-if=\"choferes.length > 0\">\n        <h1 class=\"text-center mb-3\"> Lista de Choferes</h1>\n\n        <div class=\"form-inline mb-3\">\n            <label for=\"search-input\" class=\"mr-2\">Búsqueda:</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"search\" id=\"search-input\" />\n        </div>\n\n        <table class=\"table table-striped table-bordered\">\n            <thead>\n                <th>Chofer ID</th>\n                <th>Chofer Nombre</th>\n                <th>Chofer Apellido</th>\n                <th>Chofer Documento</th>\n            </thead>\n            <tbody>\n                <tr v-for=\"chofer in filteredChoferes\" v-bind:key=\"chofer.id\">\n                    <td> {{ chofer.id }} </td>\n                    <td> {{ chofer.nombre }} </td>\n                    <td> {{ chofer.apellido }} </td>\n                    <td> {{ chofer.documento }} </td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n</template>\n<script>\n\nimport ChoferService from '@/services/ChoferService';\n\nexport default {\n    name: '',\n    data() {\n        return {\n            choferes: [],\n            token: '',\n            requestComplete: false,\n        }\n    },\n    computed: {\n        async filteredChoferes() {\n            filteredChoferes =  await choferes.filter(chofer => chofer.nombre.toLowerCase().includes(this.search.toLowerCase()));\n            return filteredChoferes\n        },\n    },\n    methods: {\n\n        async getChoferes() {\n            try {\n                // Obtener un token llamando al método \"getTokens\" importado\n\n                // Llamar al método \"getCamiones\" del servicio de camión, pasando el token como un parámetro\n                this.choferes = await ChoferService.getChoferes(this.token);\n                this.requestComplete = true;\n            } catch (error) {\n                console.error(error);\n            }\n        }\n    },\n    // created(){\n    //      this.getCamiones()\n    // }\n\n}\n\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}