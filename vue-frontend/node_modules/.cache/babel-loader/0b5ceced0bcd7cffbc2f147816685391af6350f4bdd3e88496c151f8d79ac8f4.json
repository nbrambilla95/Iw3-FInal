{"ast":null,"code":"import CamionService from '@/services/CamionService';\n\n// Importar la clase Token\nimport Token from '@/components/Token.vue';\nexport default {\n  name: '',\n  data() {\n    return {\n      camiones: [],\n      token: ''\n    };\n  },\n  methods: {\n    async getCamiones() {\n      try {\n        // Obtener un token llamando al método \"getTokens\" importado\n        const token = await Token.default.getTokens();\n\n        // Llamar al método \"getCamiones\" del servicio de camión, pasando el token como un parámetro\n        const response = await CamionService.getCamiones(token);\n        this.camiones = response.data;\n      } catch (error) {\n        console.error(error);\n      }\n    }\n  }\n};\n// created(){\n//      this.getCamiones()\n// }","map":{"version":3,"mappings":"AA8BA,OAAOA,aAAY,MAAO,0BAA0B;;AAEpD;AACA,OAAOC,KAAI,MAAO,wBAAwB;AAI1C,eAAe;EACXC,IAAI,EAAE,EAAE;EAERC,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE;IACX,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,WAAW,GAAG;MAChB,IAAI;QACA;QACA,MAAMF,KAAI,GAAI,MAAMJ,KAAK,CAACO,OAAO,CAACC,SAAS,EAAE;;QAE7C;QACA,MAAMC,QAAO,GAAI,MAAMV,aAAa,CAACO,WAAW,CAACF,KAAK,CAAC;QACvD,IAAI,CAACD,QAAO,GAAIM,QAAQ,CAACP,IAAI;MACjC,EAAE,OAAOQ,KAAK,EAAE;QACZC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACxB;IACJ;EACJ;AACJ,CAAC;AACG;AACA;AACA","names":["CamionService","Token","name","data","camiones","token","methods","getCamiones","default","getTokens","response","error","console"],"sourceRoot":"","sources":["/home/nicolas/Downloads/repos/Iw3-FInal/vue-frontend/src/components/Camion.vue"],"sourcesContent":["<template>\n    <button id=\"id_button_camiones\" @click=\"getCamiones\">\n        Listar camiones\n    </button>\n    <div>\n        <input type=\"text\" v-model=\"token\" />\n    </div>\n    <div class=\"container\" v-if=\"camiones.length > 0\">\n        <h1 class=\"text-center\"> Lista de Camiones</h1>\n        <table class=\"table table-striped\">\n            <thead>\n                <th>Camion ID</th>\n                <th>Camion Patente</th>\n                <th>Camion Descripcion</th>\n                <th>Camion Cisternado</th>\n            </thead>\n            <tbody>\n                <tr v-for=\"camion in camiones\" v-bind:key=\"camion.id\">\n                    <td> {{ camion.id }} </td>\n                    <td> {{ camion.patente }} </td>\n                    <td> {{ camion.descripcion }} </td>\n                    <td> {{ camion.cisternado }} </td>\n                </tr>\n            </tbody>\n        </table>\n\n    </div>\n</template>\n\n<script>\nimport CamionService from '@/services/CamionService';\n\n// Importar la clase Token\nimport Token from '@/components/Token.vue';\n\n\n\nexport default {\n    name: '',\n\n    data() {\n        return {\n            camiones: [],\n            token: ''\n        };\n    },\n    methods: {\n        async getCamiones() {\n            try {\n                // Obtener un token llamando al método \"getTokens\" importado\n                const token = await Token.default.getTokens();\n\n                // Llamar al método \"getCamiones\" del servicio de camión, pasando el token como un parámetro\n                const response = await CamionService.getCamiones(token);\n                this.camiones = response.data;\n            } catch (error) {\n                console.error(error);\n            }\n        }\n    }\n};\n    // created(){\n    //      this.getCamiones()\n    // }\n\n\n\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}