{"ast":null,"code":"import Navbar from './Navbar.vue';\nimport Camion from './Camion.vue';\nimport Orden from './Orden.vue';\nimport Chofer from './Chofer.vue';\nimport Login from './Login.vue';\nexport default {\n  name: 'MainPage',\n  components: {\n    Navbar,\n    Camion,\n    Orden,\n    Chofer,\n    Login\n  },\n  data() {\n    return {\n      selectedComponent: '' // Aquí se almacenará el nombre del componente seleccionado\n    };\n  },\n\n  mounted() {\n    // Por defecto, se cargará el componente 1\n    this.selectedComponent = 'Login';\n  },\n  methods: {\n    // Esta función se llamará desde la barra de navegación para cambiar el componente cargado\n    selectComponent(componentName) {\n      this.selectedComponent = componentName;\n    }\n  }\n};","map":{"version":3,"mappings":"AAkBA,OAAOA,MAAK,MAAO,cAAc;AACjC,OAAOC,MAAK,MAAO,cAAc;AACjC,OAAOC,KAAI,MAAO,aAAa;AAC/B,OAAOC,MAAK,MAAO,cAAc;AACjC,OAAOC,KAAI,MAAO,aAAa;AAE/B,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVN,MAAM;IACNC,MAAM;IACNC,KAAK;IACLC,MAAM;IACNC;EACF,CAAC;EACDG,IAAI,GAAG;IACL,OAAO;MACLC,iBAAiB,EAAE,EAAE,CAAE;IACzB,CAAC;EACH,CAAC;;EACDC,OAAO,GAAG;IACR;IACC,IAAI,CAACD,iBAAgB,GAAI,OAAO;EACnC,CAAC;EACDE,OAAO,EAAE;IACP;IACAC,eAAe,CAACC,aAAa,EAAE;MAC7B,IAAI,CAACJ,iBAAgB,GAAII,aAAa;IACxC;EACF;AACF,CAAC","names":["Navbar","Camion","Orden","Chofer","Login","name","components","data","selectedComponent","mounted","methods","selectComponent","componentName"],"sourceRoot":"","sources":["/home/nicolas/Downloads/repos/Iw3-FInal/vue-frontend/src/components/Main_navigator.vue"],"sourcesContent":["<template>\n  <div>\n    <div>\n      <!-- Aquí se cargará el componente seleccionado en la barra de navegación -->\n      <Navbar @selectComponent=\"selectedComponent = $event\" />\n\n      <Camion v-if=\"selectedComponent === 'Camion'\" />\n    \n     <!-- <Chofer v-if=\"selectedComponent === 'Chofer'\" />\n      <Orden v-if=\"selectedComponent === 'Orden'\" /> -->\n\n      <component :is=\"selectedComponent\"></component>\n      \n    </div>\n  </div>\n</template>\n\n<script>\nimport Navbar from './Navbar.vue';\nimport Camion from './Camion.vue';\nimport Orden from './Orden.vue';\nimport Chofer from './Chofer.vue';\nimport Login from './Login.vue';\n\nexport default {\n  name: 'MainPage',\n  components: {\n    Navbar,\n    Camion,\n    Orden,\n    Chofer,\n    Login\n  },\n  data() {\n    return {\n      selectedComponent: '', // Aquí se almacenará el nombre del componente seleccionado\n    };\n  },\n  mounted() {\n    // Por defecto, se cargará el componente 1\n     this.selectedComponent = 'Login';\n  },\n  methods: {\n    // Esta función se llamará desde la barra de navegación para cambiar el componente cargado\n    selectComponent(componentName) {\n      this.selectedComponent = componentName;\n    },\n  },\n};\n</script>\n\n<style>\nnav {\n  background-color: #333;\n  color: #fff;\n  display: flex;\n  justify-content: space-between;\n}\n\nnav a {\n  color: #fff;\n  text-decoration: none;\n  padding: 0.5em 1em;\n  display: inline-block;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}