{"ast":null,"code":"import CamionService from '@/services/CamionService';\nimport TokenService from '@/services/TokenService';\nexport default {\n  name: '',\n  components: {\n    TokenService\n  },\n  data() {\n    return {\n      camiones: [],\n      token: ''\n    };\n  },\n  methods: {\n    getCamiones() {\n      // Obtener el token llamando al método \"getTokens\" del servicio de token\n\n      // Asignar el token recibido a la variable \"token\"\n      this.token = token;\n\n      // Llamar al método \"getCamiones\" del servicio de camión, pasando el token como parámetro\n      CamionService.getCamiones(this.token).then(response => {\n        // Asignar la lista de camiones a la propiedad \"camiones\"\n        this.camiones = response.data;\n      }).catch(error => {\n        // Manejar cualquier error que pueda ocurrir\n        console.error(error);\n      });\n    }\n  }\n  // created(){\n  //      this.getCamiones()\n  // }\n};","map":{"version":3,"mappings":"AA2BA,OAAOA,aAAY,MAAO,0BAA0B;AACpD,OAAOC,YAAW,MAAO,yBAAyB;AAElD,eAAe;EACXC,IAAI,EAAE,EAAE;EAERC,UAAU,EAAE;IACRF;EACJ,CAAC;EAEDG,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE;IAEX;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,WAAW,GAAG;MACd;;MAGQ;MACA,IAAI,CAACF,KAAI,GAAIA,KAAK;;MAElB;MACAN,aAAa,CAACQ,WAAW,CAAC,IAAI,CAACF,KAAK,EAC/BG,IAAI,CAAEC,QAAQ,IAAK;QAChB;QACA,IAAI,CAACL,QAAO,GAAIK,QAAQ,CAACN,IAAI;MACjC,CAAC,EACAO,KAAK,CAAEC,KAAK,IAAK;QACd;QACAC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACxB,CAAC,CAAC;IAElB;EACJ;EACI;EACA;EACA;AAEJ","names":["CamionService","TokenService","name","components","data","camiones","token","methods","getCamiones","then","response","catch","error","console"],"sourceRoot":"","sources":["/home/nicolas/Downloads/repos/Iw3-FInal/vue-frontend/src/components/Camion.vue"],"sourcesContent":["<template>\n    <button id=\"id_button_camiones\" @click=\"getCamiones\" >\n        Listar camiones\n    </button>\n    <div class=\"container\" v-if=\"camiones.length > 0\">\n        <h1 class=\"text-center\"> Lista de Camiones</h1>\n        <table class=\"table table-striped\">\n            <thead>\n                <th>Camion ID</th>\n                <th>Camion Patente</th>\n                <th>Camion Descripcion</th>\n                <th>Camion Cisternado</th>\n            </thead>\n            <tbody>\n                <tr v-for=\"camion in camiones\" v-bind:key=\"camion.id\">\n                    <td> {{ camion.id }} </td>\n                    <td> {{ camion.patente }} </td>\n                    <td> {{ camion.descripcion }} </td>\n                    <td> {{ camion.cisternado }} </td>\n                </tr>\n            </tbody>\n        </table>\n\n    </div>\n</template>\n\n<script>\nimport CamionService from '@/services/CamionService';\nimport TokenService from '@/services/TokenService';\n\nexport default {\n    name: '',\n\n    components: {\n        TokenService\n    },\n\n    data() {\n        return {\n            camiones: [],\n            token: '',\n\n        }\n    },\n    methods: {\n        getCamiones() {\n        // Obtener el token llamando al método \"getTokens\" del servicio de token\n\n            \n                // Asignar el token recibido a la variable \"token\"\n                this.token = token;\n\n                // Llamar al método \"getCamiones\" del servicio de camión, pasando el token como parámetro\n                CamionService.getCamiones(this.token)\n                    .then((response) => {\n                        // Asignar la lista de camiones a la propiedad \"camiones\"\n                        this.camiones = response.data;\n                    })\n                    .catch((error) => {\n                        // Manejar cualquier error que pueda ocurrir\n                        console.error(error);\n                    });\n            \n    }\n},\n    // created(){\n    //      this.getCamiones()\n    // }\n\n}\n\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}