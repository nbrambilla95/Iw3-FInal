{"ast":null,"code":"import LoginService from '@/services/LoginService';\nexport default {\n  name: '',\n  data() {\n    return {\n      username: '',\n      password: '',\n      token: \"\",\n      requestComplete: false,\n      error: null\n    };\n  },\n  methods: {\n    async getTokens() {\n      try {\n        if (!this.username || !this.password) {\n          this.error = 'Debes ingresar un nombre de usuario y una contraseña';\n          return;\n        }\n        this.token = await LoginService.getTokens(this.username, this.password);\n        this.requestComplete = true;\n        return this.token;\n      } catch (error) {\n        this.error = error;\n        console.error(error);\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAqBA,OAAOA,YAAW,MAAO,yBAAwB;AAEjD,eAAe;EACbC,IAAI,EAAE,EAAE;EAERC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,eAAe,EAAE,KAAK;MACtBC,KAAK,EAAE;IAET;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,SAAS,GAAG;MAChB,IAAI;QACF,IAAI,CAAC,IAAI,CAACN,QAAO,IAAK,CAAC,IAAI,CAACC,QAAQ,EAAE;UACpC,IAAI,CAACG,KAAI,GAAI,sDAAsD;UACnE;QACF;QAEA,IAAI,CAACF,KAAI,GAAI,MAAML,YAAY,CAACS,SAAS,CAAC,IAAI,CAACN,QAAQ,EAAE,IAAI,CAACC,QAAQ,CAAC;QAEvE,IAAI,CAACE,eAAc,GAAI,IAAI;QAE3B,OAAO,IAAI,CAACD,KAAI;MAClB,EAAE,OAAOE,KAAK,EAAE;QACd,IAAI,CAACA,KAAI,GAAIA,KAAK;QAElBG,OAAO,CAACH,KAAK,CAACA,KAAK,CAAC;MACtB;IACF;EACF;AACF,CAAC","names":["LoginService","name","data","username","password","token","requestComplete","error","methods","getTokens","console"],"sourceRoot":"","sources":["/home/nicolas/Downloads/repos/Iw3-FInal/vue-frontend/src/components/Login.vue"],"sourcesContent":["<template>\n  <div style=\"display: flex; justify-content: center;\" v-if=\"!requestComplete\">\n    <button id=\"id_button_token\" @click=\"getTokens\" class=\"btn btn-primary\">Solicitar Token</button>\n\n    <label for=\"input_username\">Usuario: </label>\n    <input type=\"text\" id=\"input_username\" v-model=\"username\">\n\n    <label for=\"input_password\">Constrasenia: </label>\n    <input type=\"password\" id=\"input_password\" v-model=\"password\">\n\n  </div>\n\n  <p v-if=\"error\" class=\"error-message\">{{ error }}</p>\n\n\n  <!-- Añadir un <label> para mostrar el token -->\n  <label id=\"label_token\" v-if=\"requestComplete\">Token: {{ token }}</label>\n\n</template>\n\n<script>\nimport LoginService from '@/services/LoginService'\n\nexport default {\n  name: '',\n\n  data() {\n    return {\n      username: '',\n      password: '',\n      token: \"\",\n      requestComplete: false,\n      error: null\n\n    }\n  },\n  methods: {\n    async getTokens() {\n      try {\n        if (!this.username || !this.password) {\n          this.error = 'Debes ingresar un nombre de usuario y una contraseña';\n          return;\n        }\n\n        this.token = await LoginService.getTokens(this.username, this.password);\n\n        this.requestComplete = true;\n\n        return this.token\n      } catch (error) {\n        this.error = error;\n\n        console.error(error);\n      }\n    }\n  }\n};\n\n</script>\n\n<style>\n#id_button_token {\n  display: flex;\n  justify-content: center;\n}\n\n#label_token {\n  display: block;\n  margin-top: 50px;\n  border: 1px solid black;\n  content: 'Token:';\n}\n\n.error-message {\n  color: red;\n\n  border: 1px solid rgb(255, 0, 0);\n  display: block;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}