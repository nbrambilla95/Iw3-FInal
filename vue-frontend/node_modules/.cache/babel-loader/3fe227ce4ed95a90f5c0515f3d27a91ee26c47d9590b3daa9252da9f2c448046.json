{"ast":null,"code":"export default {\n  name: 'LoginForm',\n  data() {\n    return {\n      username: '',\n      password: '',\n      token: \"\",\n      requestComplete: false,\n      error: null\n    };\n  },\n  methods: {\n    async getTokens() {\n      try {\n        if (!this.username || !this.password) {\n          this.error = 'Debes ingresar un nombre de usuario y una contraseña';\n          return;\n        }\n        this.token = await LoginService.getTokens(this.username, this.password);\n        this.requestComplete = true;\n        return this.token;\n      } catch (error) {\n        this.error = error;\n        console.error(error);\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAkCA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,eAAe,EAAE,KAAK;MACtBC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,SAAS,GAAG;MAChB,IAAI;QACF,IAAI,CAAC,IAAI,CAACN,QAAO,IAAK,CAAC,IAAI,CAACC,QAAQ,EAAE;UACpC,IAAI,CAACG,KAAI,GAAI,sDAAsD;UACnE;QACF;QAEA,IAAI,CAACF,KAAI,GAAI,MAAMK,YAAY,CAACD,SAAS,CAAC,IAAI,CAACN,QAAQ,EAAE,IAAI,CAACC,QAAQ,CAAC;QAEvE,IAAI,CAACE,eAAc,GAAI,IAAI;QAE3B,OAAO,IAAI,CAACD,KAAI;MAClB,EAAE,OAAOE,KAAK,EAAE;QACd,IAAI,CAACA,KAAI,GAAIA,KAAK;QAElBI,OAAO,CAACJ,KAAK,CAACA,KAAK,CAAC;MACtB;IACF;EACF;AACF,CAAC","names":["name","data","username","password","token","requestComplete","error","methods","getTokens","LoginService","console"],"sourceRoot":"","sources":["/home/nicolas/Downloads/repos/Iw3-FInal/vue-frontend/src/components/Login.vue"],"sourcesContent":["<template>\n  <form @submit.prevent=\"login\" class=\"container\">\n    <div class=\"form-row\" v-if=\"!requestComplete\">\n      <div class=\"form-group col-md-6\">\n        <label for=\"email\">Nombre de usuario:</label>\n        <input id=\"email\" type=\"email\" v-model=\"username\" class=\"form-control\" />\n      </div>\n\n      <div class=\"form-group col-md-6\">\n        <label for=\"password\">Contraseña:</label>\n        <input id=\"password\" type=\"password\" v-model=\"password\" class=\"form-control\" />\n      </div>\n    </div>\n\n    <div class=\"form-row\">\n      <div class=\"form-group col-md-6\">\n        <button type=\"submit\" class=\"btn btn-primary \" @click=\"getTokens\">Iniciar sesión</button>\n      </div>\n\n      <!-- <div class=\"form-group col-md-6\">\n        <a @click=\"showSignupForm\" class=\"btn btn-link\">Crear una nueva cuenta</a>\n      </div> -->\n    </div>\n\n    <p v-if=\"error\" class=\"error-message\">{{ error }}</p>\n\n    <!-- Añadir un <label> para mostrar el token -->\n    <label id=\"label_token\" v-if=\"requestComplete\">Token: {{ token }}</label>\n\n\n  </form>\n</template>\n\n<script>\nexport default {\n  name: 'LoginForm',\n  data() {\n    return {\n      username: '',\n      password: '',\n      token: \"\",\n      requestComplete: false,\n      error: null\n    };\n  },\n  methods: {\n    async getTokens() {\n      try {\n        if (!this.username || !this.password) {\n          this.error = 'Debes ingresar un nombre de usuario y una contraseña';\n          return;\n        }\n\n        this.token = await LoginService.getTokens(this.username, this.password);\n\n        this.requestComplete = true;\n\n        return this.token\n      } catch (error) {\n        this.error = error;\n\n        console.error(error);\n      }\n    }\n  }\n};\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}