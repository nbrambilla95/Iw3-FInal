</th>\n                <th>Chofer Apellido</th>\n                <th>Chofer Documento</th>\n\n            </thead>\n            <tbody>\n                <tr v-for = \"orden in ordenes\" v-bind:key = \"orden.id\">\n                <td> {{ orden.id }} </td>\n                <td> {{ orden.fechaRecepcion }} </td>\n                <td> {{ orden.fechaRecepcionPesajeInicial }} </td>\n                <td> {{ orden.fechaFinCarga }} </td>\n                </tr>\n            </tbody>\n        </table>\n\n    </div>\n</template>\n\n<script>\nimport ChoferService from '@/services/ChoferService';\n\n    export default{\n        name: '',\n        data(){\n            return {\n                choferes : []\n            }\n        },\n        methods:{\n            getChoferes(){\n                ChoferService.getChoferes().then((response) => {\n                    this.choferes = response.data\n                })\n            }\n        },\n        // created(){\n        //      this.getCamiones()\n        // }\n\n    }\n\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}